{
    "name": "edt",
    "version": "3.15.5-hotfix-v2",
    "dateVersion": "27/09/2024",
    "licence": "MIT",
    "dependencies": {
        "@inseefr/lunatic": "2.0.7-v2",
        "@inseefrlab/lunatic-edt": "^0.13.4",
        "@mui/material": "^6.0.2",
        "@mui/styles": "^6.0.2",
        "@mui/utils": "^6.0.2",
        "@react-pdf/renderer": "^3.4.4",
        "axios": "^1.7.7",
        "dexie": "^4.0.8",
        "dotenv-cli": "^7.4.2",
        "elasticlunrjs": "^1.0.1",
        "i18next": "^23.14.0",
        "lodash": "^4.17.21",
        "oidc-react": "^3.2.2",
        "react": "^18.3.1",
        "react-device-detect": "^2.2.3",
        "react-dom": "^18.3.1",
        "react-error-boundary": "^4.0.13",
        "react-i18next": "^15.0.1",
        "react-responsive": "^10.0.0",
        "react-router-dom": "^6.26.1",
        "tss-react": "^4.9.13",
        "uuidv4": "^6.2.13",
        "web-vitals": "^4.2.3"
    },
    "resolutions": {
        "react-error-overlay": "6.0.9",
        "@react-pdf/font": "2.2.1",
        "@react-pdf/fontkit": "2.0.2",
        "@react-pdf/pdfkit": "2.1.0"
    },
    "devDependencies": {
        "@eslint/js": "^9.9.0",
        "@types/lodash": "^4.17.7",
        "@types/react": "^18.3.3",
        "@types/react-dom": "^18.3.0",
        "@vitejs/plugin-react": "^4.3.1",
        "concurrently": "^9.0.0",
        "eslint": "^9.9.0",
        "eslint-plugin-react-hooks": "^5.1.0-rc.0",
        "eslint-plugin-react-refresh": "^0.4.9",
        "globals": "^15.9.0",
        "prettier": "^3.3.3",
        "typescript": "^5.5.3",
        "typescript-eslint": "^8.0.1",
        "vite": "^5.4.1",
        "vite-plugin-node-polyfills": "^0.22.0",
        "vite-plugin-pwa": "^0.20.3",
        "vite-plugin-svgr": "^4.2.0"
    },
    "scripts": {
        "dev": "vite",
        "check": "tsc -b",
        "build": "tsc -b && vite build",
        "lint": "eslint .",
        "preview": "vite preview --port 3000",
        "build:staging": "dotenv -e .env.staging -- tsc -b && vite build",
        "build:production": "dotenv -e .env.production -- tsc -b && vite build",
        "start-with-lib": "concurrently \"yarn run dev\" \"cd ./node_modules/@inseefrlab/lunatic-edt && tsc-watch\""
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ]
    },
    "lint-staged": {
        "*.{ts,tsx}": [
            "eslint --fix"
        ],
        "*.{ts,tsx,json,md}": [
            "prettier --write"
        ]
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged -v"
        }
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    }
}
